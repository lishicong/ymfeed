<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<classPathEntry
		location="/Users/lishicong/Documents/mywork/niux/workspace/ymfeed/WebContent/WEB-INF/lib/mysql-connector-java-5.1.41-bin.jar" />

	<context id="MysqlTable" targetRuntime="MyBatis3">

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/ymfeed_db?useUnicode=true&amp;characterEncoding=utf-8"
			userId="root" password="123456" />

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.sc.ymfeed.mybatis.dto"
			targetProject="ymfeed">
			<property name="rootClass" value="com.sc.ymfeed.mybatis.support.BaseObject" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.sc.ymfeed.mybatis.dto.sql"
			targetProject="ymfeed" />

		<javaClientGenerator targetPackage="com.sc.ymfeed.mybatis.dao"
			targetProject="ymfeed" type="XMLMAPPER" />

		<!-- tables -->
		<table schema="user_account" tableName="user_account"
			domainObjectName="UserAccount" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
			<columnOverride column="DISABLED" property="disabled"
				javaType="Boolean" jdbcType="TINYINT" />
			<columnOverride column="BLOCKED" property="blocked"
				javaType="Boolean" jdbcType="TINYINT" />
			<columnOverride column="ACTIVE" property="active"
				javaType="Boolean" jdbcType="TINYINT" />
		</table>
		<table schema="user_persistent" tableName="user_persistent"
			domainObjectName="UserPersistent" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
		<table schema="feed_info" tableName="feed_info"
			domainObjectName="FeedInfo" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
			<columnOverride column="CONTENT" property="content"
				javaType="String" jdbcType="VARCHAR" />
		</table>
		<table schema="feed_comment" tableName="feed_comment"
			domainObjectName="FeedComment" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
		<table schema="comment_reply" tableName="comment_reply"
			domainObjectName="CommentReply" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
	</context>
</generatorConfiguration>